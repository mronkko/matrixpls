	Keywords = {Partial least squares, statistical and methodological myths and urban legends, STRUCTURAL equation modeling},
	Language = {en},
	Month = mar,
	Number = {3},
	Pages = {425--448},
	Title = {A critical examination of common beliefs about partial least squares path modeling},
